{
  "name": "feedmail",
  "version": "1.0.0",
  "description": "An Application designed to gather survey data from users",
  "main": "index.js",
  "engines": {
    "node": "11.10.1",
    "npm": "6.7.0"
  },
  "scripts": {
    "start": "node ./server",
    "server": "nodemon ./server",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DavidEngelstad/FeedMail.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DavidEngelstad/FeedMail/issues"
  },
  "homepage": "https://github.com/DavidEngelstad/FeedMail#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^5.0.0",
    "cookie-session": "^1.3.3",
    "express": "^4.17.0",
    "lodash": "^4.17.19",
    "mongoose": "^5.7.7",
    "nodemon": "^1.19.4",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^2.0.0",
    "path-parser": "^6.1.0",
    "sendgrid": "^5.2.3",
    "stripe": "^7.13.0"
  }
}
